{
  "name": "vue-flatpickr-component",
  "version": "4.0.0",
  "description": "Vue.js component for Flatpickr date-time picker",
  "main": "dist/vue-flatpickr.min.js",
  "unpkg": "dist/vue-flatpickr.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankurk91/vue-flatpickr-component.git"
  },
  "keywords": [
    "vue",
    "flatpickr",
    "vue-flatpickr",
    "datepicker"
  ],
  "author": "ankurk91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankurk91/vue-flatpickr-component/issues"
  },
  "homepage": "https://github.com/ankurk91/vue-flatpickr-component",
  "scripts": {
    "test": "node ./node_modules/jest/bin/jest.js",
    "lint": "eslint --ext .js,.vue --fix src",
    "start": "npm run dev",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=webpack.config.dev.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --hide-modules -p --progress",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "flatpickr": "^4.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "^4.0.0-beta",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-vue": "^0.8.1",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.5",
    "style-loader": "^0.19.0",
    "unminified-webpack-plugin": "^1.2.0",
    "vee-validate": "^2.0.0-rc.12",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "vue-test-utils": "^1.0.0-beta.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "engines": {
    "node": ">= 4.2.0",
    "npm": ">= 3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "mapCoverage": true,
    "collectCoverage": true
  }
}
